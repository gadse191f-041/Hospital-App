        android:layout_x="121dp"
        android:layout_y="417dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="+94 76 570 8969"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="italic"
