<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContactUsActivity"
    android:background="@color/white">

    <TextView
        android:id="@+id/txtHospitalName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="5dp"
        android:layout_y="10dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Family Love Hospital"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/txtSub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="30dp"
        android:layout_y="66dp"
        android:drawableStart="@drawable/ic_baseline_contact_phone_24"
        android:drawablePadding="10dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Contact Us"
        android:textColor="#5529CD"
        android:textSize="27sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="118dp" />

    <ImageView
        android:id="@+id/imgLogo"
        android:layout_width="141dp"
        android:layout_height="108dp"
        android:layout_x="267dp"
        android:layout_y="9dp"
